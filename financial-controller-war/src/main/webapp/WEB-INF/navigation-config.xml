<?xml version="1.0"?>
<faces-config version="2.0" xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xi="http://www.w3.org/2001/XInclude" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">

	<navigation-rule>
		<from-view-id>/*</from-view-id>
		<navigation-case>
			<from-outcome>cadastro_layout</from-outcome>
			<to-view-id>/cadastroLayout.xhtml</to-view-id>
		</navigation-case>

		<navigation-case>
			<from-outcome>cadastro_campo</from-outcome>
			<to-view-id>/cadastroCampo.xhtml</to-view-id>
		</navigation-case>
		
		<navigation-case>
			<from-outcome>meusTestes</from-outcome>
			<to-view-id>/meusTestes.xhtml</to-view-id>
		</navigation-case>
		
	</navigation-rule>
	
</faces-config>