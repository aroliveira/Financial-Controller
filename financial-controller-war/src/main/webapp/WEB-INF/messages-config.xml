<?xml version="1.0"?>
<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
      http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
	version="2.0">
	<application>
	
		<locale-config>
			<default-locale>pt</default-locale>
			<supported-locale>en</supported-locale>
		</locale-config>
		
		<resource-bundle>
			<base-name>aroliveira.messages.importacao.importacaoMessages</base-name>
			<var>msg_importacao</var>
		</resource-bundle>
		<message-bundle>aroliveira.messages.importacao.importacaoMessages</message-bundle>
		
	</application>
</faces-config>